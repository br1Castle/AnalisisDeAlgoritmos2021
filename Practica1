#include<stdlib.h>
#include<stdio.h>

 int main(){
 	int num, ban=1, aux, i, j, k, p;
 	
 	printf("Cuantos numeros quieres ordenar?");
 	scanf("%d", &num);
 	int fila[num];
 	
 	fila[0]= rand() % 99;
 	printf("->%d\n", fila[0]);
 	
 	for(i=1;i<num;i++){
 		int numero = rand() % 99;
 		printf("->%d\n", numero);
 		
		for(j=0;j<i;j++){
			if(numero<fila[j]){
				int poner=numero;
				aux = fila[j];
				for(k=j;k<i;k++){
					fila[k]=poner;
					poner=aux;
					aux=fila[k+1];		
				}
				fila[k]=poner;
				ban=0;
			}
		}
		if(ban)
			fila[i]=numero;
		ban=1;		
	}
	
	for(i=0;i<num;i++){
		printf("=>%d\n", fila[i]);
	}
	 
	 
	 
	 
	return 0;
 }
